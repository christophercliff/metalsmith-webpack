{
  "name": "metalsmith-webpack",
  "version": "1.0.3",
  "description": "A webpack plugin for Metalsmith",
  "keywords": [
    "metalsmith",
    "javascript",
    "webpack"
  ],
  "author": "Christopher Cliff <ctcliff@gmail.com>",
  "repository": {
    "type": "git",
    "url": "http://github.com/christophercliff/metalsmith-webpack"
  },
  "license": "MIT",
  "main": "./lib/",
  "scripts": {
    "test": "mocha -r should --reporter spec"
  },
  "dependencies": {
    "debug": "^2.6.6",
    "enhanced-resolve": "^0.7.6",
    "webpack": "^2.5.1"
  },
  "devDependencies": {
    "assert-dir-equal": "^1.0.1",
    "babel-eslint": "^7.2.3",
    "eslint": "^3.19.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-babel": "^4.1.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-node": "^4.2.2",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "metalsmith": "^1.0.1",
    "mocha": "^2.0.1",
    "mocha-eslint": "^3.0.1",
    "nyc": "^10.3.2",
    "should": "^4.3.0"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": "standard",
    "installedESLint": true,
    "plugins": [
      "standard",
      "babel"
    ],
    "env": {
      "node": true,
      "mocha": true
    }
  },
  "babel": {
    "presets": [
      "stage-0"
    ],
    "plugins": [
      "add-module-exports"
    ],
    "env": {
      "test": {
        "presets": [
          "es2015-node6"
        ],
        "plugins": [
          "istanbul"
        ]
      },
      "node4": {
        "presets": [
          "es2015-node4"
        ]
      },
      "node6": {
        "presets": [
          "es2015-node6"
        ]
      }
    }
  },
  "nyc": {
    "include": [
      "lib/**/*.js"
    ],
    "require": [
      "babel-register"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "directories": {
    "test": "test"
  }
}
